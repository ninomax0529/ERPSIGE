	<queryString language="SQL">
		<![CDATA[SELECT


(select  logo from unidad_de_negocio where codigo=$P{unidad_negocio}  )  as  logo ,


  art.nombre as servicio,c.nombre as cliente
,ct.numero_de_contrato as num_contrato,dc.fecha_ultimo_pago_desde,

dc.fecha_suspendido,dc.razon_actualizacion,dc.nombre_usuario
from  cliente c
INNER JOIN contrato_de_servicio ct on c.codigo=ct.cliente
INNER JOIN  detalle_contrato_de_servicio  dc on ct.codigo=dc.contrato_de_servicio
INNER JOIN articulo art  on art.codigo=dc.equipo

where   dc.habilitado=false    
and   date(dc.fecha_suspendido)   between     $P{fecha_desde}  and     $P{fecha_hasta}

order by  dc.fecha_suspendido ]]>
	</queryString>
