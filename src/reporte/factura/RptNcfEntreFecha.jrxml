	<queryString language="SQL">
		<![CDATA[SELECT  


(select empresa from configuracion ) as empresa,
(select telefono from configuracion ) as telefono_empresa,
(select direccion from configuracion ) as direccion_empresa,
f.ncf,f.fecha, f.ncf_fecha_valido_hasta,t.descripcion as tipo_ncf, ug.nombre as unidad_negocio,f.anulada
 
  FROM   factura f INNER JOIN unidad_de_negocio ug  
  on ug.codigo=f.unidad_de_negocio
   INNER JOIN tipo_ncf t on f.tipo_ncf=t.codigo

 where  

f.fecha   between    $P{fecha_desde}   and     $P{fecha_hasta}   order by  f.ncf,f.fecha,t.codigo]]>
	</queryString>
