	<queryString language="SQL">
		<![CDATA[SELECT  

det.codigo as factura,
det.cantidad,
nombre_articulo,
art.numero,
det.precio,
det.itbis,
det.sub_total,
det.total ,
det.descripcion_pago



FROM    detalle_factura det, articulo art ,unidad u

where    det.articulo = art.codigo  

and  u.codigo=det.unidad

and    det.factura= $P{factura}

]]>
