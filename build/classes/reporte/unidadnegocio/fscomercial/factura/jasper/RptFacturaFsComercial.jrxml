	<queryString language="SQL">
		<![CDATA[SELECT 

 (select  logo from unidad_de_negocio where codigo= 3 )  as  logo ,
 (select  nombre  from unidad_de_negocio where codigo=3 )  as empresa,
 
  (select  direccion  from unidad_de_negocio  where codigo=3 )  as  direccion_ung,

(select telefono from configuracion ) as telefono_empresa,
(select direccion from configuracion ) as direccion_empresa,
 (select  rnc  from unidad_de_negocio  where codigo=3  )  as  RNC,
(select email from configuracion ) as email,
ncf_fecha_valido_hasta  as  fecha_valido_hasta ,
(select  ung.email  from unidad_de_negocio ung  where ung.codigo=3 )  as  correo_ung,
(select  ung.telefono  from unidad_de_negocio ung  where ung.codigo=3 )  as  telefono_ung ,
 f.codigo,f.fecha,c.nombre as cliente,c.direccion,c.telefono,
 f.tipo_venta,
 f.numero,f.ncf,
   c.rnc as rnc_cliente,
   f.vendedor,
   u.nombre as  usuario,f.tipo_ncf,f.concepto
     
   FROM   factura f,cliente c,usuario u
  
 where  
 
 c.codigo=f.cliente  
 and u.codigo=f.usuario
 and f.codigo= $P{factura}]]>
	</queryString>
