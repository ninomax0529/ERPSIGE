	<parameter name="unidad_negocio" class="java.lang.Integer"/>
	<parameter name="sqlParam" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT 

(select  logo from unidad_de_negocio  where codigo= $P{unidad_negocio} )  as logo ,

p.nombre as proyecto,
f.ncf,
 f.numero as num_fact,f.fecha,sub_total,itbis,total,isr,


s.descripcion as suplidor

  
  FROM  factura_suplidor f  INNER JOIN 

   suplidor s on   s.codigo=f.suplidor 

    INNER JOIN proyecto p on  p.codigo=f.proyecto 
  
 where   
 f.unidad_de_negocio= $P{unidad_negocio} 
 
 
 and   f.fecha   between   $P{fechaIni}   and   $P{fechafin} 
 
   $P!{sqlParam}    order by   p.nombre]]>
	</queryString>
