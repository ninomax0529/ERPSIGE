	<queryString language="SQL">
		<![CDATA[SELECT 

(select  logo from unidad_de_negocio where codigo=$P{unidad_negocio}  )  as  logo ,

 ( select  nombre  from unidad_de_negocio  where  codigo=$P{unidad_negocio} )  as empresa,

(select rnc from unidad_de_negocio ung  where ung.codigo=$P{unidad_negocio}  ) as  rnc_ung,
(select email from configuracion ) as email,
(select  ung.email  from unidad_de_negocio ung    where  ung.codigo= $P{unidad_negocio}  )  as  correo_ung,
(select  ung.telefono  from unidad_de_negocio ung   where  ung.codigo= $P{unidad_negocio}  )  as  telefono_ung ,

ri.fecha,ri.codigo,ri.codigo,ri.monto,
c.nombre,c.direccion,c.telefono,ri.codigo as numero

FROM  reconciliacion_interna_cliente  ri  ,cliente  c

where   ri.anulada=false 

and   c.codigo=ri.cliente

 and  ri.codigo = $P{reconciliacion}]]>
	</queryString>
