	<queryString language="SQL">
		<![CDATA[SELECT 
 (select  logo from unidad_de_negocio where codigo= $P{unidad_negocio} )  as  logo ,
ri.codigo,
  ri.fecha,ri.numero,
  c.nombre,c.codigo as cod_cliente,
  ri.tipo_recibo,

case when ri.tipo_recibo='AV' THEN   ri.monto else  0 end as monto,
 
( SELECT IFNULL(sum(monto_pagado),0) from detalle_recibo_ingreso dr where dr.recibo=ri.codigo ) as cobrado

 from recibo_ingreso ri 
  INNER JOIN cliente c
  on  c.codigo=ri.cliente 

  where   ri.anulado=false  and 

   ri.fecha  between       $P{fechaIni}  and   $P{fechaFin} 
 
   $P!{sqlParam}]]>
	</queryString>
