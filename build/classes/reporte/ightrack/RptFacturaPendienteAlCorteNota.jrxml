	<queryString language="SQL">
		<![CDATA[SELECT * from (
	-- java.io.InputStream
 SELECT 
 (select  logo from unidad_de_negocio where codigo= $P{unidad_negocio} )  as  logo ,
c.nombre,c.codigo,f.ncf,f.fecha,f.total,f.numero,
       DATEDIFF(NOW(),f.fecha_vencimiento) as dias_vencimiento,

 round( f.total-(  	
  
  (  SELECT IFNULL(sum(total),0)  from recibo_ingreso ri INNER JOIN  detalle_recibo_ingreso dri 
on  ri.codigo=dri.recibo  where  ri.anulado=false  and  factura=f.codigo     $P!{sqlParam}   ) +

( SELECT IFNULL(sum(monto),0) from nota_credito nc 
 where nc.anulada=false and  nc.factura=f.codigo  and nc.tipo_nc=1  and  tipo_documento=7  $P!{sqlParam} )+
   
  ( SELECT  ifnull(sum(drci.credito),0)  as monto FROM  reconciliacion_interna_cliente rci  inner join 
    detalle_reconciliacion_interna_cliente drci on drci.reconciliacion=rci.codigo  and  rci.anulada=false 
    where rci.anulada=false  and drci.tipo_documento=7  and drci.documento=f.codigo  $P!{sqlParam} ) 

 ),2)   as  pendiente,

( SELECT IFNULL(sum(total),0)  from recibo_ingreso  ri  INNER JOIN  detalle_recibo_ingreso dri 
on  ri.codigo=dri.recibo   where  ri.anulado=false   and   factura=f.codigo     $P!{sqlParam}   )+

( SELECT IFNULL(sum(monto),0) from nota_credito nc 
 where nc.anulada=false and  nc.factura=f.codigo  and nc.tipo_nc=1  and  tipo_documento=7  $P!{sqlParam} )+
   
  ( SELECT  ifnull(sum(drci.credito),0)  as monto FROM  reconciliacion_interna_cliente rci  inner join 
    detalle_reconciliacion_interna_cliente drci on drci.reconciliacion=rci.codigo  and  rci.anulada=false 
    where rci.anulada=false  and drci.tipo_documento=7  and drci.documento=f.codigo  $P!{sqlParam} )  
 as abono

 from cliente  c 
 
 INNER  JOIN  factura f  on  c.codigo=f.cliente 
  where    f.tipo_venta=2
    and  f.anulada=false     $P!{sqlCliente} 
     
 order by   c.nombre , f.fecha  )  as  r   $P!{sqlParamPendienteCero}]]>
