	<parameter name="sqlParam" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT 

(select empresa from configuracion ) as empresa,
(select telefono from configuracion ) as telefono_empresa,
(select direccion from configuracion ) as direccion_empresa,


nombre,porciento_comision,

sum(total) as total,sum(pagado) as pagado,sum(pendiente) as pendiente,
round((sum(pagado)*porciento_comision)/100,2) as comision

 from (
SELECT 
cg.nombre as cargo,
ej.nombre,ej.porciento_comision,
f.ncf,f.nombre_cliente,f.fecha,
f.total,(SELECT IFNULL(sum(dri.total),0) as monto from recibo_ingreso ri INNER JOIN detalle_recibo_ingreso dri
  on ri.codigo=dri.recibo  where ri.anulado=false  and f.codigo=dri.factura  
  
   and  ri.fecha    BETWEEN         $P{fecha_desde}   and    $P{fecha_hasta}  )  as pagado,

f.total-(SELECT IFNULL(sum(dri.total),0) as monto from recibo_ingreso ri INNER JOIN detalle_recibo_ingreso dri
  on ri.codigo=dri.recibo  where ri.anulado=false  and f.codigo=dri.factura  
   and ri.fecha BETWEEN        $P{fecha_desde}   and    $P{fecha_hasta}
)  as pendiente

 from  factura  f  INNER JOIN ejecutivo_de_venta ej 

 on  ej.codigo=f.vendedor
 INNER JOIN cargo cg on ej.cargo=cg.codigo

  
 where  f.fecha <=$P{fecha_hasta}   and   f.anulada=false 
 and origen_factura=1    $P!{sqlParam}  )  as  r  where  pagado>0
 
 GROUP BY  nombre
 
 order by   nombre]]>
	</queryString>
