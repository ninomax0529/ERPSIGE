<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXCheckBox?>
<?import com.jfoenix.controls.JFXDatePicker?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="491.0" prefWidth="956.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.contrato.reporte.RptEstadoContratoController">
   <children>
      <HBox prefHeight="29.0" prefWidth="889.0" spacing="10.0">
         <children>
            <HBox spacing="5.0">
               <children>
                  <Label alignment="BOTTOM_LEFT" prefHeight="25.0" prefWidth="86.0" text="Fecha  desde" />
                  <JFXDatePicker fx:id="dpFechaInicio" disable="true" prefHeight="25.0" prefWidth="140.0">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </JFXDatePicker>
                  <Label alignment="BOTTOM_LEFT" prefHeight="25.0" prefWidth="83.0" text="Fecha  hasta" />
                  <JFXDatePicker fx:id="dpFechaFinal" disable="true" prefHeight="25.0" prefWidth="143.0">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </JFXDatePicker>
                  <JFXCheckBox fx:id="chEntreFecha" onAction="#chEntreFechaActionEvent" prefHeight="29.0" prefWidth="101.0" text="Entre Fecha " />
                  <JFXCheckBox fx:id="chalCorte" layoutX="511.0" layoutY="10.0" onAction="#chalCorteActionEvent" prefHeight="29.0" prefWidth="79.0" text="Al Corte" />
               </children>
            </HBox>
            <JFXCheckBox fx:id="chDetalldo" onAction="#chDetalldoActtionevent" prefHeight="29.0" prefWidth="93.0" text="Detallado" />
            <JFXCheckBox fx:id="chResumido" layoutX="715.0" layoutY="10.0" onAction="#chResumidoActionEvent" prefHeight="29.0" prefWidth="93.0" text="Resumido" />
            <JFXButton fx:id="btnVerReporte" onAction="#btnVerReporteActionEvent" prefHeight="29.0" prefWidth="95.0" styleClass="btn-new" stylesheets="@../../../style/style.css" text="Reporte">
               <HBox.margin>
                  <Insets left="5.0" right="5.0" />
               </HBox.margin>
            </JFXButton>
         </children>
         <VBox.margin>
            <Insets bottom="5.0" left="10.0" right="5.0" top="5.0" />
         </VBox.margin>
      </HBox>
      <Separator prefWidth="200.0">
         <VBox.margin>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </VBox.margin>
         <padding>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </padding>
      </Separator>
      <JFXCheckBox fx:id="chRptServicioCliente" onAction="#chRptServicioClienteActionEvent" prefHeight="29.0" prefWidth="311.0" text=" Reporte  de Servicios   y  Gps  por  Cliente         ">
         <VBox.margin>
            <Insets left="10.0" />
         </VBox.margin>
      </JFXCheckBox>
      <VBox>
         <children>
            <JFXCheckBox fx:id="chActivo" onAction="#chActivoActionEvent" prefHeight="29.0" prefWidth="117.0" text="Activo" />
            <JFXCheckBox fx:id="chSuspendidos" onAction="#chSuspendidosActionEvent" prefHeight="29.0" prefWidth="119.0" text="Suspendido" />
            <JFXCheckBox fx:id="chTodos" onAction="#chTodosActionEvent" prefHeight="29.0" prefWidth="113.0" selected="true" text="Todos" />
            <JFXCheckBox fx:id="chPorVencer" layoutX="10.0" layoutY="10.0" onAction="#chPorVencerActionEvent" prefHeight="29.0" prefWidth="117.0" text="Por Vencer" />
            <JFXCheckBox fx:id="chVencido" layoutX="10.0" layoutY="97.0" onAction="#chVencidoActionEvent" prefHeight="29.0" prefWidth="117.0" text="Vencido" />
         </children>
         <VBox.margin>
            <Insets left="10.0" />
         </VBox.margin>
      </VBox>
   </children>
</VBox>
